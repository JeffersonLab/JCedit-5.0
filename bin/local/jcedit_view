#!/bin/csh -f

#Starts JCedit interface (limited version)
#author vhg 30-Aug-2013

setenv CLASSPATH "../../../build/common/jar/*"

java -Xms200m -Xmx500m  -Dsun.java2d.pmoffscreen=false  -Dorg.xml.sax.parser="com.sun.org.apache.xerces.internal.parsers.SAXParser" \
-Djavax.xml.parsers.DocumentBuilderFactory="com.sun.org.apache.xerces.internal.jaxp.DocumentBuilderFactoryImpl" \
-Djavax.xml.parsers.SAXParserFactory="com.sun.org.apache.xerces.internal.jaxp.SAXParserFactoryImpl" \
org.jlab.coda.cedit.cooldesktop.CDesktopLimited $1

#java -Xms200m -Xmx500m -Dcom.sun.management.jmxremote org.jlab.coda.cedit.cooldesktop.CDesktopLimited $1
